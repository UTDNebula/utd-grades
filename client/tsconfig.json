{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../tsconfig.build.json",
  "include": ["./**/*.ts", "./**/*.tsx"],
  "exclude": [], // work around https://github.com/vercel/next.js/issues/35857
  "compilerOptions": {
    "rootDir": "src",
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "jsx": "preserve",
    "resolveJsonModule": true
  }
}
